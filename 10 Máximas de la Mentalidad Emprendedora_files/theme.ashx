/* Add responsiveness to embedded videos */
.responsive-video iframe {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.responsive-video {
  position: relative;
  padding-bottom: 56.25%; /* Default for 1600x900 videos 16:9 ratio*/
  padding-top: 0px;
  height: 0;
  overflow: hidden;
}

/* Custom Footer Styles */
.custom-footer {
  background: #fff;
  padding: 30px 20px 60px;
}

.custom-footer .copyright {
  width: 100%;
  text-align: center;

}

.custom-footer .copyright .item {
  padding: 5px;
}

.custom-footer .copyright a.item {
  color: #b7b8b9;
  text-decoration: underline;
}

.custom-footer .copyright a.item:hover,
.custom-footer .copyright a.item:focus {
  text-decoration: none;
}

.custom-footer .logo-section {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  text-align: center;
  margin-bottom: 40px;
  padding: 20px;
  width: 100%;
}

.custom-footer .logo-section img {
  display: inline-block;
  max-height: 100px;
  max-width: 100%;
  margin: 10px 25px;
}

#footerPolicy a.text-muted {
  font-size: 14px;
  margin: 10px auto 40px;
  color: #b7b8b9;
  text-decoration: underline;
}

