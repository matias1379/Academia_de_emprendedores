/* CoursePlayer Kursdetailsansicht */

#coursePlayerWrapper .navbar-header {
  display: flex;
  align-content: center;
  justify-content: space-around;
  width: 60%;
  max-width: 1200px;
}

#coursePlayerWrapper a.m-nav-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
}

/* exit */
#coursePlayerWrapper a.m-nav-item:first-of-type {
  order: 4;
}

#coursePlayerWrapper a.m-nav-item:first-of-type .m-nav-btn {
  text-transform: capitalize !important;
}

#coursePlayerWrapper a.m-nav-item:nth-of-type(2) {
  order: 2;
}

