  text-transform: lowercase !important;
}

#coursePlayerWrapper a.m-nav-item:nth-of-type(3) {
  order: 1;
}

#coursePlayerWrapper a.m-nav-item:nth-of-type(3) .m-nav-btn {
  text-transform: lowercase !important;
}

/* feedback */
